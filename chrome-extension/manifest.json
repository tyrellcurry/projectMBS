{
  "name": "Marketo Local Browser Support",
  "description": "A Google Chrome Extension for supporting Marketo Guided Templates locally.",
  "version": "0.0.3",
  "manifest_version": 3,
  "icons": {
    "128": "mbs-icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "include_globs": ["*127.*", "*file://*", "*localhost:*"],
      "js": ["mbs.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
